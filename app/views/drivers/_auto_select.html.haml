.panel.panel-success#rate_select
  .panel-heading
    = t('app.autos.slct-button')
  .panel-body
    .form-group.row
      .col-md-4
        = f.label :auto
      .col-md-8
        = select(:driver, :auto_id, Auto.all.collect {|p| [ (p.model + ' - ' + p.number), p.id ] }, { include_blank: true }, class: 'form-control')